{
  "name": "DigiPassKeyLocker",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios12": "react-native run-ios --simulator='iPhone 12 Pro'",
    "iosIPadPro": "react-native run-ios --simulator='iPad Pro (11-inch) (3rd generation)'",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && pod install && cd ..",
    "clean": "npm run clean-android && npm run clean-ios",
    "android-release": "react-native run-android --variant=release",
    "fast": "npm install && npm run clean && npm run android && npm run ios12",
    "fastPad": "npm install && npm run clean && npm run android && npm run iosIPadPro",
    "fast-release": "npm install && npm run clean-android && npm run android-release"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/netinfo": "^9.3.10",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "axios": "^1.5.0",
    "base-64": "^1.0.0",
    "bs58": "^5.0.0",
    "moment": "^2.29.4",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-gesture-handler": "^2.7.0",
    "react-native-image-helper": "^0.0.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-paper": "^5.10.3",
    "react-native-reanimated": "^2.9.1",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.25.0",
    "react-native-toasty": "^2.0.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-vector-icons": "^9.0.0",
    "realm": "^10.18.0",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/base-64": "^1.0.2",
    "@types/jest": "^29.5.4",
    "@types/react": "^18.2.21",
    "@types/react-native": "^0.72.2",
    "@types/react-test-renderer": "^18.0.0",
    "@types/text-encoding": "^0.0.39",
    "@typescript-eslint/eslint-plugin": "^3.10.0",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "^17.0.2",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
